	"maintainers": [
		{
			"name": "Arshad Yaseen",
			"email": "m@arshadyaseen.com",
			"url": "https://arshadyaseen.com"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/arshad-yaseen/bunup.git"
	},
	"funding": "https://github.com/sponsors/arshad-yaseen",
	"homepage": "https://bunup.dev",
	"keywords": ["bun", "bunup", "bun-bundler"],
	"files": ["dist"],
	"dependencies": {
		"chokidar": "^4.0.3",
		"coffi": "^0.1.28",
		"oxc-resolver": "^5.0.1",
		"oxc-transform": "^0.58.1",
		"rolldown": "1.0.0-beta.7",
		"rolldown-plugin-dts": "^0.9.4",
		"tinyexec": "^1.0.1",
		"ts-import-resolver": "^0.1.16"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./plugins": {
			"types": "./dist/plugins.d.ts",
			"require": "./dist/plugins.cjs",
			"import": "./dist/plugins.js"
		}
	},
	"bin": {
		"bunup": "./dist/cli.js"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"scripts": {
		"npm-publish": "bun publish --access public --no-git-checks",
		"prepack": "cp ../../LICENSE .",
		"postpack": "rm LICENSE"
	},
	"type": "module"
}
