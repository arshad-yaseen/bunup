{
    "name": "bunup",
    "description": "âš¡ A blazing-fast build tool for your libraries built with Bun.",
    "version": "0.5.0",
    "license": "MIT",
    "author": "Arshad Yaseen <m@arshadyaseen.com> (https://arshadyaseen.com)",
    "maintainers": [
        {
            "name": "Arshad Yaseen",
            "email": "m@arshadyaseen.com",
            "url": "https://arshadyaseen.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arshad-yaseen/bunup.git"
    },
    "funding": "https://github.com/sponsors/arshad-yaseen",
    "homepage": "https://bunup.dev",
    "keywords": ["bun", "bunup", "bun-bundler"],
    "files": ["dist", "./../../LICENSE"],

    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "dependencies": {
        "chokidar": "^4.0.3",
        "coffi": "^0.1.25",
        "oxc-resolver": "^5.0.1",
        "oxc-transform": "^0.58.1",
        "rolldown": "1.0.0-beta.7",
        "rolldown-plugin-dts": "^0.7.0",
        "tinyexec": "^1.0.1",
        "ts-import-resolver": "^0.1.10"
    },
    "peerDependencies": {
        "typescript": ">=4.5.0"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./plugins": {
            "import": {
                "types": "./dist/plugins.d.ts",
                "default": "./dist/plugins.js"
            },
            "require": {
                "types": "./dist/plugins.d.cts",
                "default": "./dist/plugins.cjs"
            }
        }
    },
    "bin": {
        "bunup": "./dist/cli.js"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "scripts": {
        "npm-publish": "bun publish --access public --no-git-checks"
    },
    "type": "module",
    "types": "./dist/index.d.ts"
}
