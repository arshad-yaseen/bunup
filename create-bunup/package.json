{
    "name": "create-bunup",
    "description": "Quickly scaffold a new TypeScript package with Bunup",
    "version": "0.4.72",
    "license": "MIT",
    "author": "Arshad Yaseen <m@arshadyaseen.com> (https://arshadyaseen.com)",
    "maintainers": [
        {
            "name": "Arshad Yaseen",
            "email": "m@arshadyaseen.com",
            "url": "https://arshadyaseen.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arshad-yaseen/bunup.git"
    },
    "homepage": "https://bunup.dev/typescript-library-starter",
    "funding": "https://github.com/sponsors/arshad-yaseen",
    "keywords": ["bunup", "typescript", "cli", "bun", "create", "starter"],
    "files": ["dist"],
    "bin": {
        "create-bunup": "dist/index.mjs"
    },
    "scripts": {
        "fetch-latest-dep-versions": "bun run fetch-latest-dep-versions.ts && biome format --write src/latest-dep-versions.json",
        "release": "release-it --ci",
        "release:dry-run": "bun run release --dry-run"
    },
    "release-it": {
        "git": false,
        "npm": { "publish": false },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular",
                "infile": "CHANGELOG.md"
            }
        }
    },
    "dependencies": {
        "@clack/prompts": "^0.7.0",
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@types/node": "^20.17.30",
        "typescript": "^5.8.2"
    }
}
